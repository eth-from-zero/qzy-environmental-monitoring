import {
    HorizontalBox, SpinBox
} from "std-widgets.slint";

TextSpinBox := HorizontalBox {
    property <string> text;
    property <int> value;
    property <bool> enabled;

    t := Text {
        text: root.text;
    }

    sb := SpinBox {
        value <=> root.value;
        minimum: 0;
        maximum: 100;
        enabled <=> root.enabled;
    }
}

